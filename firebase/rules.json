{
  "rules": {
    "_admin": {
      ".read": "true"
    },
    "backend": {
      "chatlock": {
        ".read": "true"
      }
    },
    "chat": {
      ".read": "true"
    },
    "playlists": {
      "$uid": {
        ".read": "auth != null && auth.uid == $uid",
        ".write": "auth != null && auth.uid == $uid"
      }
    },
    "playing_media": {
      ".read": "true"
    },
    "presence": {
      ".read": "true",
      "$username": {
        ".write": "auth != null && auth.uid == newData.child('uid').val() || auth != null && auth.uid == data.child('uid').val()"
      }
    },
    "users": {
      "$uid": {
        ".read": "true",
        "private": {
          ".read": "auth != null && auth.uid == $uid"
        }
      }
    },
    "moderator": {
      ".read": "true"
    },
    "bans": {
      ".read": "true"
    },
    "waitlist": {
      "tasks": {
        ".read": "true"
      }
    }
  }
}
