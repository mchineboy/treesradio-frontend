{
  "rules": {
    "_admin": {
      ".read": "true"
    },
    "backend": {
      "chatlock": {
        ".read": "true"
      }
    },
    "chat": {
      ".read": "true"
    },
    "playlists": {
      "$uid": {
        ".read": "auth != null && auth.uid == $uid",
        ".write": "auth != null && auth.uid == $uid"
      }
    },
    "playing_media": {
      ".read": "true"
    },
    "presence": {
      ".read": "true",
      "$username": {
        ".write": "newData.parent().parent().child('users/' + auth.uid + '/username').val() == $username"
      }
    },
    "users": {
      "$uid": {
        ".read": "true",
        "private": {
          ".read": "auth != null && auth.uid == $uid"
        }
      }
    },
    "moderator": {
      ".read": "true"
    },
    "bans": {
      ".read": "true"
    },
    "waitlist": {
      "tasks": {
        ".read": "true"
      }
    },
    "queues": {
      ".write": "auth != null",
      "chat": {
        "tasks": {
          ".indexOn": [
            "_state"
          ]
        }
      }
    }
  }
}
