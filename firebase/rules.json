{
  "rules": {
    "_admin": {
      ".read": "true"
    },
    "backend": {
      "chatlock": {
        ".read": "true"
      }
    },
    "chat": {
      ".read": "true"
    },
    "playlists": {
      "$uid": {
        ".read": "auth != null && auth.uid == $uid",
        ".write": "auth != null && auth.uid == $uid"
      }
    },
    "playing": {
      ".read": "true"
    },
    "presence": {
      ".read": "true",
      "$username": {
        ".write": "newData.parent().parent().child('users/' + auth.uid + '/username').val() == $username"
      }
    },
    "users": {
      "$uid": {
        ".read": "true"
      }
    },
    "private": {
      "$uid": {
        ".read": "auth != null && auth.uid == $uid",
        ".write": "auth != null && auth.uid == $uid"
      }
    },
    "moderator": {
      ".read": "true"
    },
    "bans": {
      ".read": "true"
    },
    "waitlist": {
      "tasks": {
        ".read": "true"
      }
    },
    "queues": {
      "chat": {
        "tasks": {
          ".indexOn": [
            "_state"
          ],
          "$task": {
            ".write": "auth != null && auth.uid == newData.child('uid').val() || auth != null && auth.uid == data.child('uid').val()"
          }
        }
      }
    },
    "ranks": {
      ".read": "true"
    },
    "avatars": {
      ".read": "true",
      "$username": {
        ".write": "newData.parent().parent().child('users/' + auth.uid + '/username').val() == $username"
      }
    }
  }
}
