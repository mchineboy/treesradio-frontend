{
  "rules": {
    "_admin": {
      ".read": "true"
    },
    "events": {
      ".read": "true"
    },
    "backend": {
      "public": {
        ".read": "true"
      }
    },
    "chat": {
      ".read": "true"
    },
    "playlists": {
      "$uid": {
        ".read": "auth != null && auth.uid == $uid",
        ".write": "auth != null && auth.uid == $uid"
      }
    },
    "playing": {
      ".read": "true"
    },
    "presence": {
      ".read": "true",
      "$uid": {
        ".write": "auth != null && auth.uid == $uid"
      }
    },
    "users": {
      "$uid": {
        ".read": "true"
      }
    },
    "private": {
      "$uid": {
        ".read": "auth != null && auth.uid == $uid",
        ".write": "auth != null && auth.uid == $uid"
      }
    },
    "user_events": {
      "$uid": {
        ".read": "auth != null && auth.uid == $uid"
      }
    },
    "moderator": {
      ".read": "true"
    },
    "bans": {
      ".read": "true"
    },
    "waitlist": {
      ".read": "true"
    },
    "ranks": {
      ".read": "true"
    },
    "avatars": {
      ".read": "true",
      "$uid": {
        ".write": "auth != null && auth.uid == $uid"
      }
    },
    "event_bus": {
      "$event": {
        ".write": "auth != null && auth.uid == newData.child('uid').val() || auth != null && auth.uid == data.child('uid').val()"
      }
    }
  }
}
