{
  "name": "treesradio-frontend",
  "version": "0.1.0",
  "description": "TreesRadio Frontend",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "check-dependencies --install && webpack-dev-server --inline --colors --content-base public/ --output-public-path=assets/",
    "lint": "eslint app/**",
    "bolt-compile": "firebase-bolt firebase/security.bolt --output firebase/rules.json",
    "bolt-deploy": "npm run bolt-compile && firebase deploy:rules -f treesradio-dev",
    "bolt-deploy-live": "npm run bolt-compile && firebase deploy:rules -f treesradio-dev",
    "update-env": "firebase data:set /_admin/env firebase/env.json -y -f treesradio-live",
    "build-prod": "npm run bolt-compile && webpack -p --progress --colors --config webpack.config.build.js",
    "ci-build-bolt": "npm run bolt-compile",
    "ci-build-app": "webpack -p --display-error-details --config webpack.config.build.js",
    "ci-build-prod": "npm run bolt-compile && webpack -p --display-error-details --config webpack.config.build.js",
    "deploy-surge": "webpack -p --progress --colors --config webpack.config.build.js && surge public treesradio.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zbuttram/treesradio-frontend.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/zbuttram/treesradio-frontend/issues"
  },
  "homepage": "https://github.com/zbuttram/treesradio-frontend#readme",
  "devDependencies": {
    "babel-core": "^6.7.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "check-dependencies": "^0.12.0",
    "css-loader": "^0.25.0",
    "eslint": "^3.5.0",
    "eslint-plugin-react": "^6.2.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "firebase-bolt": "^0.8.0",
    "firebase-tools": "^3.0.7",
    "less": "^2.6.1",
    "less-loader": "^2.2.2",
    "node-sass": "^3.4.1",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14"
  },
  "dependencies": {
    "axios": "^0.14.0",
    "babel-polyfill": "^6.7.4",
    "classnames": "^2.2.1",
    "favico.js": "^0.3.10",
    "font-awesome": "^4.5.0",
    "font-awesome-webpack": "0.0.4",
    "jquery": "^3.1.0",
    "lodash": "^4.6.1",
    "mention-completer": "^1.0.8",
    "moment": "^2.12.0",
    "node-buzz": "^1.1.4",
    "querystring": "^0.2.0",
    "re-base": "^1.5.1",
    "react": "^15.0.0",
    "react-dom": "^15.0.0",
    "react-emoji": "^0.4.4",
    "react-image-fallback": "^3.2.0",
    "react-linkify": "^0.1.1",
    "react-player": "^0.9.0",
    "react-progressbar": "^1.2.0",
    "react-slider": "^0.7.0",
    "sweetalert": "^1.1.3",
    "url": "^0.11.0"
  }
}
